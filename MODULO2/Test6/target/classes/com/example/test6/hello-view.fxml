    <HBox alignment="BOTTOM_RIGHT" spacing="5.0" xmlns:fx="http://javafx.com/fxml">
        <Button text="Pulsame 2B" onAction="#onPulsame2B"/>
        <Button text="Pulsame 2A" onAction="#onPulsame2A"/>
