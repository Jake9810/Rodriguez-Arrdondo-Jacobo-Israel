    <Label fx:id="welcomeText"/>
    <HBox alignment="TOP_LEFT" spacing="5.0" xmlns:fx="http://javafx.com/fxml">
        <Button text="Pulsame1B" onAction="#onPulsame1B"/>
        <Button text="Pulsame 1A" onAction="#onPulsame1A"/>
